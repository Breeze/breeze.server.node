  "scripts": {
    "clean": "rimraf msmodels && rimraf mymodels && rimraf mymeta.json && rimraf msmeta.json",
    "test": "mocha -b ModelMapper.test.js",
    "debug": "node --inspect-brk node_modules/mocha/bin/mocha -b",
    "auto": "sequelize-auto",
