  "dependencies": {
    "bluebird": "^3.7.2",
    "breeze-client": ">=2.0.4",
    "breeze-sequelize": "file:../../src/breeze-sequelize-0.5.5.tgz",
    "lodash": "^4.17.15",
    "mysql": "^2.18.0",
